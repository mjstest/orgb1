      <ProgramUnit Name="LOGOUT_ATTENDANCE" ProgramUnitText="PROCEDURE LOGOUT_ATTENDANCE IS&amp;#10;cnt number;&amp;#10;BEGIN&amp;#10;if :GLOBAL.CHECKOUT='Y' then&#x9;&amp;#10;  insert into EMP_ATTENDANCE &amp;#10;(emp_id, &amp;#10;emp_timing, &amp;#10;emp_date, &amp;#10;in_out)&amp;#10;values( :global.emp_id,to_char(sysdate,'HH24:MI'),to_date(sysdate),3);&amp;#10;forms_ddl('commit');&amp;#10;&#x9;message('GOOD BYE '|| :global.ename);&amp;#10;&#x9;--&#x9;message('GOOD BYE '|| :global.ename);&amp;#10;&#x9;else&amp;#10;&#x9;exit_form;&amp;#10;end if;&amp;#10;END;&amp;#10;" ProgramUnitType="Procedure"/>
