#x9; set_alert_property(v_alert,&amp;#10;&#x9;&#x9;                    alert_message_text,&amp;#10;&#x9;&#x9;                    'Wronge User ID Or PASSWORD, Please Try Again.');&amp;#10;     N := show_alert(v_alert);&amp;#10;&#x9; &#x9; --RAISE FORM_TRIGGER_FAILURE;&amp;#10; &amp;#10;&#x9;   v_count := 1 ;&amp;#10;&#x9;   :parameter.P_TRIAL := NVL (:parameter.P_TRIAL , 0) + V_COUNT ;&amp;#10;&amp;#10;/*RESTRICT USERS LOGIN ATTEMPTES BY 3 ATTEMP ONLY AFTER THIS EXIT FORM*/&amp;#10;&#x9;&amp;#10;&#x9;  IF :parameter.P_TRIAL = 3 then     -- START OF IFSTATEMENT NO 3&amp;#10;&#x9;  &#x9;&amp;#10;&#x9;&#x9;  N := SHOW_ALERT('LOGIN_ART') ;&amp;#10;&#x9;&#x9;  &amp;#10;&#x9;&#x9;  EXIT_FORM(NO_VALIDATE) ;&amp;#10;&#x9;&#x9;  &amp;#10; &#x9;  END IF;                            -- END OF IFSTATEMENT NO 3&amp;#10;&#x9;&amp;#10;&amp;#10;  END IF;               -- END OF IF STATEMENT NO 2&amp;#10;                -- END OF IF STATEMENT NO 1&amp;#10;&amp;#10;&amp;#10;EXCEPTION&amp;#10;       &#x9;&amp;#10;   WHEN NO_DATA_FOUND THEN &amp;#10;   set_alert_property(v_alert,&amp;#10;                      alert_message_text,&amp;#10;                      'Please Choose your Name from List then Enter your Password');&amp;#10;   N :=  show_alert(v_alert);&amp;#10;   RAISE FORM_TRIGGER_FAILURE ;&amp;#10;   &amp;#10;   WHEN OTHERS THEN&amp;#10;   MESSAGE (ERROR_CODE || ' ' || ERROR_TYPE || ' ' || ERROR_TEXT);&amp;#10;   MESSAGE (ERROR_CODE || ' ' || ERROR_TYPE || ' ' || ERROR_TEXT);&amp;#10;   RAISE FORM_TRIGGER_FAILURE ;&amp;#10;END;&amp;#10;&amp;#10;" ProgramUnitType="Procedure"/>
